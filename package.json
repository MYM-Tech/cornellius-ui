{
  "name": "@mym-tech/cornellius-ui",
  "version": "0.0.2",
  "description": "MYM's Frontend Components library.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MYM-Tech/frontend-library.git"
  },
  "author": "MYM Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MYM-Tech/frontend-library/issues"
  },
  "homepage": "https://github.com/MYM-Tech/frontend-library#readme",
  "scripts": {
    "postinstall": "rm -rf ./node_modules/@types/react",
    "install": "yarn postinstall",
    "test": "vitest run",
    "release": "release-it",
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "lint:markup": "vue-tsc",
    "lint:style": "stylelint src/**/*.{css,scss,vue} --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/vue3": "^6.5.4",
    "@testing-library/vue": "^6.5.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "babel-loader": "^8.2.4",
    "chromatic": "^6.5.4",
    "classnames": "^2.3.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "happy-dom": "^2.55.0",
    "lazy-load-vue3": "^1.2.8",
    "postcss": "8",
    "release-it": "^14.13.1",
    "sass": "^1.50.1",
    "stylelint": "^14.7.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.8"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/runtime-core": "^3.2.33",
    "classnames": "^2.3.1",
    "normalize-scss": "^7.0.1",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "vite": "^2.9.2",
    "vite-plugin-css-injected-by-js": "^1.4.0",
    "vitest": "^0.9.4",
    "vue": "^3.2.25"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/cornellius-ui.umd.js",
  "module": "./dist/cornellius-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/cornellius-ui.es.js",
      "require": "./dist/cornellius-ui.umd.js"
    }
  }
}
