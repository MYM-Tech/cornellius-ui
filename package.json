{
  "name": "cornellius",
  "version": "0.0.1",
  "description": "MYM's Frontend Components library.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MYM-Tech/frontend-library.git"
  },
  "author": "MYM Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MYM-Tech/frontend-library/issues"
  },
  "homepage": "https://github.com/MYM-Tech/frontend-library#readme",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "release-it",
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:markup": "vue-tsc --noEmit",
    "lint:style": "stylelint src/**/*.{css,scss,vue}"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-loader": "^8.2.4",
    "classnames": "^2.3.1",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "release-it": "^14.13.1",
    "sass": "^1.50.1",
    "stylelint": "^14.7.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.2",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/cornellius-ui.umd.js",
  "module": "./dist/cornellius-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/cornellius-ui.es.js",
      "require": "./dist/cornellius-ui.umd.js"
    }
  }
}
